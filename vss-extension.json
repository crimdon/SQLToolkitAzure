{
    "manifestVersion": 1,
    "id": "sql-toolkit-azure",
    "name": "SQL Toolkit for Azure",
    "version": "1.6.5",
    "public": true,
    "publisher": "andrewlackenby",
    "categories": [
        "Build and release"
    ],
    "links": {
        "support": {
            "uri": "https://github.com/crimdon/SQLToolkitAzure/issues"
        },
        "learn": {
            "uri": "https://github.com/crimdon/SQLToolkitAzure/wiki"
        },
        "repository": {
            "uri": "https://github.com/crimdon/SQLToolkitAzure"
        },
        "privacypolicy": {
            "uri": "https://github.com/crimdon/SQLToolkitAzure/wiki/Privacy-Policy"
        }
    },
    "repository": {
        "type": "git",
        "uri": "https://github.com/crimdon/SQLToolkitAzure"
    },
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "description": "SQL Toolkit for Azure",
    "icons": {
        "default": "icon.jpg"
    },
    "content": {
        "details": {
            "path": "readme.md"
        },
        "license": {
            "path": "LICENSE.txt"
        }
    },
    "files": [
        {
            "path": "RunStoredProcedureAzure"
        },
        {
            "path": "RunSingleSqlScriptAzure"
        },
        {
            "path": "RunSqlScriptsAzure"
        },
        {
            "path": "AddOrUpdateUserAzure"
        }
    ],
    "contributions": [
        {
            "id": "runstoredprocedureazure",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "RunStoredProcedureAzure"
            }
        },
        {
            "id": "runsinglesqlscriptazure",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "RunSingleSqlScriptAzure"
            }
        },
        {
            "id": "runsqlscriptsazure",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "RunSqlScriptsAzure"
            }
        },
        {
            "id": "addorupdateuserazure",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "AddOrUpdateUserAzure"
            }
        }
    ]
}